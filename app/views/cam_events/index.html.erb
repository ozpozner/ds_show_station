<h1>Listing cam_events</h1>

<table>
  <thead>
    <tr>
      <th>Source</th>
      <th>Eventtime</th>
      <th>Eventtype</th>
      <th>Acknowledged</th>
      <th>Acknowledged at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cam_events.each do |cam_event| %>
      <tr>
        <td><%= cam_event.source %></td>
        <td><%= cam_event.eventTime %></td>
        <td><%= cam_event.eventType %></td>
        <td><%= cam_event.acknowledged %></td>
        <td><%= cam_event.acknowledged_at %></td>
        <td><%= link_to 'Show', cam_event %></td>
        <td><%= link_to 'Edit', edit_cam_event_path(cam_event) %></td>
        <td><%= link_to 'Destroy', cam_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cam event', new_cam_event_path %>
